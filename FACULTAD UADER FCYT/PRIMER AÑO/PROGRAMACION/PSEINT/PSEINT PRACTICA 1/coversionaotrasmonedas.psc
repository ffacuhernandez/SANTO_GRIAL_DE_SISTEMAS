Algoritmo coversionaotrasmonedas
	Definir pesos, dolares, euros, realess Como Real;
	Escribir "escriba la cantidad de pesos que desea convertir";
	Leer pesos;
	dolares=pesos/390;
	realess=pesos/41;
	euros=pesos/230;
	Escribir "su cantidad en dolares es: ",dolares, " ,su cantidad en reales son: ", realess, " y su cantidad en euros son: ", euros;
	
